{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**"]
        },
        "start": {
            "dependsOn": ["build"],
            "outputs": [],
            "inputs": ["src/**/*.ts"]
        },
        "test": {
            "dependsOn": ["build"],
            "outputs": [],
            "inputs": ["test/**/*.ts"]
        },
        "dev": {
            "dependsOn": ["build", "start"],
            "outputs": [],
            "inputs": []
        }
    }
}
